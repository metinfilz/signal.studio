<Application x:Class="SignalStudio.Core.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SignalStudio.Core"
             StartupUri="MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resource/Icon/Toolbar.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="Roboto">pack://application:,,,/Resource/Font/Roboto/#Roboto</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
